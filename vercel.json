{
  "version": 2,
  "builds": [
    { "src": "api/auth.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/auth.ts" }
  ],
  "rewrites": [
    {
      "source": "/form/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
